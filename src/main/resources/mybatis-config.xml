<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.yourplace.admin.login.vo.AdminVO"
			alias="myAdmin" />
		<typeAlias type="com.yourplace.admin.memberview.vo.MemberVO"
			alias="myMember" />
		<typeAlias type="com.yourplace.host.login.vo.HostVO"
			alias="myHost" />
		<typeAlias type="com.yourplace.custom.login.vo.UserVO"
			alias="myUser" />
	</typeAliases>
	<mappers>
		<mapper resource="mappings/test-mapper.xml" />
		<mapper resource="mappings/host/host-login-mapping.xml" />
		<mapper resource="mappings/custom/custom-login-mapping.xml" />
		<mapper resource="mappings/admin/admin-login-mapping.xml" />
		<mapper resource="mappings/admin/admin-allview-mapping.xml" />
	</mappers>
</configuration>